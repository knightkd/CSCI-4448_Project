<p id="notice"><%= notice %></p>

<h1>Listing Partner Lists</h1>

<table>
  <thead>
    <tr>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @partner_lists.each do |partner_list| %>
      <tr>
        <td><%= partner_list.organization %></td>
        <td><%= link_to 'Show', partner_list %></td>
        <td><%= link_to 'Edit', edit_partner_list_path(partner_list) %></td>
        <td><%= link_to 'Destroy', partner_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Partner list', new_partner_list_path %>
